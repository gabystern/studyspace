<h3> Available Study Rooms </h3>

<% @study_rooms.each do |study_room|  %>

<ul>
  <li> <b> <%= link_to study_room.name, study_room_path(study_room.id) %> </b> </li>
  <p> <b> Study Category: </b> <%= study_room.category.name %> <p>
  <p> <b> Location: </b> <%= study_room.location.name %> </P>
</ul>
<% end %>

