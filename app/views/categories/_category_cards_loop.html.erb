<%@categories.each do |category| %>
    <div class='col s12 m4 center-align'>
        <%=link_to category_path(category.slug) do%>
            <div class='card grey lighten-2 hoverable'>
                <div class='card-content'>
                    <span class='card-title'><%=category.name%></span>
                </div>
            </div>
        <%end%>
    </div>
<% end %>
